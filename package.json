{
    "name": "@yookue/ant-buddy-pro",
    "version": "0.0.4",
    "title": "Ant Buddy Pro",
    "description": "Awesome components based on React & Ant Design Pro",
    "homepage": "https://yookue.github.io/ant-buddy-pro",
    "organization": "Yookue Ltd",
    "private": false,
    "license": "MIT",
    "keywords": [
        "ant-buddy",
        "ant-buddy-pro",
        "antd",
        "ant-design",
        "ant-design-pro",
        "admin",
        "dashboard"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "unpkg": "dist/umd/ant-buddy-pro.min.js",
    "types": "dist/esm/index.d.ts",
    "sideEffects": [
        "**/*.less"
    ],
    "scripts": {
        "start": "set NODE_OPTIONS=--openssl-legacy-provider && dumi dev",
        "build": "father build",
        "rebuild": "del-cli ./dist && father build",
        "build:watch": "father dev",
        "docs:build": "set NODE_OPTIONS=--openssl-legacy-provider && dumi build",
        "docs:rebuild": "del-cli ./docs-dist && npm run docs:build",
        "docs:deploy": "gh-pages -d ./docs-dist",
        "doctor": "father doctor",
        "lint": "npm run lint:es && npm run lint:css",
        "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
        "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
        "prepublish": "father doctor && npm run build",
        "test": "father test",
        "test:coverage": "father test --coverage"
    },
    "dependencies": {
        "@ant-design/pro-components": "^1.1.25",
        "@babel/runtime": "^7.22.10",
        "screenfull": "^6.0.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@umijs/lint": "^4.0.0",
        "babel-plugin-import": "^1.13.8",
        "del-cli": "^5.0.0",
        "dumi": "^1.1.53",
        "eslint": "^8.46.0",
        "father": "^4.3.1",
        "gh-pages": "^5.0.0",
        "lint-staged": "^13.2.3",
        "prettier": "^2.7.1",
        "prettier-plugin-organize-imports": "^3.0.0",
        "prettier-plugin-packagejson": "^2.2.18",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-helmet-async": "^1.3.0",
        "stylelint": "^15.10.2",
        "typescript": "^5.1.6"
    },
    "optionalDependencies": {
        "@ant-design/icons": "^4.8.1"
    },
    "peerDependencies": {
        "react": ">=16.9.0",
        "react-dom": ">=16.9.0"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "lint-staged": {
        "*.{md,json}": [
            "prettier --write --no-error-on-unmatched-pattern"
        ],
        "*.{css,less}": [
            "stylelint --fix",
            "prettier --write"
        ],
        "*.{js,jsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --parser=typescript --write"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yookue/ant-buddy-pro.git"
    },
    "bugs": {
        "url": "https://github.com/yookue/ant-buddy-pro/issues"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "author": {
        "name": "Yookue Ltd",
        "email": "yookue@outlook.com",
        "url": "https://yookue.com"
    },
    "contributors": [
        {
            "name": "Davis Hsing",
            "email": "davidhsing@163.com",
            "url": "https://github.com/davidhsing"
        }
    ]
}
